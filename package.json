{
	"name": "lispcms-ui",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "NODE_ENV=production concurrently -n build: \"npm:build:*\"",
		"build:cljs": "shadow-cljs release app",
		"build:css": "postcss ./src/styles.css -o ./docs/css/styles.css",
		"build:html": "nbb -m lispcms.ui.tasks.html",
		"dev": "npm run build:html && npx concurrently -n dev: \"npm:dev:*\"",
		"dev:css": "postcss ./src/styles.css -o ./docs/dev/css/styles.css --watch",
		"dev:cljs": "shadow-cljs watch app"
	},
	"author": "Jay Zawrotny <jayzawrotny@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.3",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"nbb": "^1.2.163",
		"octokit": "^2.0.14",
		"postcss": "^8.4.21",
		"postcss-cli": "^10.1.0",
		"postcss-import": "^15.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"shadow-cljs": "^2.20.20",
		"tailwindcss": "^3.2.7"
	}
}
